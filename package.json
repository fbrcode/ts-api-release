{
  "name": "ts-api-release",
  "version": "0.4.0",
  "description": "Generic API for release automation with github action",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon api/index.ts",
    "test": "jest tests/ --verbose --detectOpenHandles",
    "build": "tsc --project ./",
    "start": "node ./dist/api/index.js",
    "docs:validate": "rdme openapi:validate",
    "prettify": "prettier --write \"**/*.ts\"",
    "test:ci": "jest --ci --verbose --coverage --reporters=default"
  },
  "author": "fbrcode",
  "license": "MIT",
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.11.15",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
